<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.bengi.calculator.MainActivity">

    <Button
        android:id="@+id/button13"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_above="@+id/button17"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="31dp"
        android:layout_marginStart="31dp"
        android:text="Button" />

    <Button
        android:id="@+id/button17"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_above="@+id/button18"
        android:layout_alignLeft="@+id/button13"
        android:layout_alignStart="@+id/button13"
        android:text="Button" />

    <Button
        android:id="@+id/button18"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_alignLeft="@+id/button17"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/button17"
        android:layout_marginBottom="106dp"
        android:text="Button" />

    <Button
        android:id="@+id/button"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_alignTop="@+id/button13"
        android:layout_toEndOf="@+id/button13"
        android:layout_toRightOf="@+id/button13"
        android:text="Button" />

    <Button
        android:id="@+id/button20"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_alignTop="@+id/button"
        android:layout_toEndOf="@+id/button"
        android:layout_toRightOf="@+id/button"
        android:text="Button" />

    <Button
        android:id="@+id/button21"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_alignTop="@+id/button20"
        android:layout_toEndOf="@+id/button20"
        android:layout_toRightOf="@+id/button20"
        android:text="Button" />

    <Button
        android:id="@+id/button24"
        android:layout_width="80sp"
        android:layout_height="80sp"
        android:layout_below="@+id/button"
        android:layout_toEndOf="@+id/button13"
        android:layout_toRightOf="@+id/button13"
        android:text="Button" />

</RelativeLayout>
